// c++ arthematic operators
#include<iostream>
using namespace std;
int main(){

// initializing variables
    int a, b; // initialized a and b
    long double sub;
    long int mul;
    long long mod;
    int sum ;
    float division;
    
    // Inputing variables
    
cin>>a;
cin>>b;
cout<<"addition of a & b: "<<a+b<<endl;

cout<<"subtraction of a & b: "<<a-b<<endl;

cout<<"multiplication of a & b: "<<a*b<<endl;

cout<<"modulus  of a & b: "<<a%b<<endl;

cout<<"division of a & b: "<<a/b<<endl;
    // operators lift shift operator and right sift opertor
    // operators
cout<<" right  shift operator "<a>>b<<endl;

cout<<"Left shift operator""<<a<<b<<endl;


return 0;
}
